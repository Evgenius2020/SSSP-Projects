void compute_matrix(int *matrix, int height, int width) {
	int *bufer = (int*)malloc(sizeof(int) * width * height);
	for (int i = 0; i < height; i++) {
		for (int j = 0; j < width; j++) {
			$1$(matrix, bufer, i, j, width, height);
		}
	}
	for (int i = 0; i < width * height; i++) {
		matrix[i] = bufer[i];
	}
	free(bufer);
}